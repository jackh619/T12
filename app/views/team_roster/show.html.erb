<%= link_to('Back', 'javascript:history.back()') %>
<div class="team roster new">
  <h2>Update member</h2>

  <%= form_for(team_roster_index_path) do |f| %>

    <table summary="book form fields">
        <tr>
            <th>First name</th>
            <td><%= @student.first_name %></td>
        </tr>
        <tr>
            <th>Last name</th>
            <td><%= @student.last_name %></td>
        </tr>
        <tr>
            <th>Class year</th>
            <td><%= @student.class_year %></td>
        </tr>
        <tr>
            <th>Email</th>
            <td><%= @student.email %></td>
        </tr>
        <tr>
            <th>Phone number</th>
            <td><%= @student.phone %></td>
        </tr>

    </table>

  <% end %>
</div>